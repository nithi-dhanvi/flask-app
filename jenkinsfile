pipeline {
    agent any

    stages {
        stage ("GIT Checkout") {
            steps {
               git branch: "main", url: "https://github.com/nithi-dhanvi/flask-app.git"

            }
        
        }
        stage ("ZIP build"){
            steps {
                zip zipFile: 'falsk-app.zip', dir: './'
            }
        }
    }

}